{
    "desc": [
        "All MuJoCo tasks with 4 seeds on each environment"
    ],
    "version": "mujoco_multibootstrap-test-Newton",
    "use_gpu": true,
    "gpus": [
        0,
        1
    ],
    "max_worker_num": 4,
    "repeats": 1,
    "parameters": {
        "create_environment.env_name": [
            "HalfCheetah-v2"
        ],
        "CriticDistributionParamNetwork.state_dependent_std": "[False]",
        "MultiBootstrapEnsemble.num_basins": "[5]",
        "MultiBootstrapEnsemble.num_particles_per_basin": "[2]",
        "MultiBootstrapEnsemble.mask_sample_ratio": "[0.5]",
        "MultiBootstrapEnsemble.initial_train_steps": "[1000]",
        "MultiBootstrapEnsemble.masked_train_steps": "[1000]",
        "MultiBootstrapEnsemble.unbiased_total_var": "[True]",
        "BayesOacAlgorithm.beta_lb": "[.5]",
        "BayesOacAlgorithm.deterministic_explore": "[False]",
        "BayesOacAlgorithm.use_q_mean_train_actor": "[True]",
        "BayesOacAlgorithm.use_basin_mean_for_target_critic": "[True]",
        "BayesOacAlgorithm.target_update_tau": "[0.005]",
        "BayesOacAlgorithm.use_epistemic_alpha": "[False]",
        "TrainerConfig.random_seed": "list(range(4))"
    }
}
