{
  "desc": "Grid Search Example For DDPG",
  "use_gpu": true,
  "gpus": [
    0,
    1
  ],
  "max_worker_num": 6,
  "repeats": 1,
  "parameters": {
    "DdpgAlgorithm.actor_optimizer": [
      "alf.optimizers.Adam(lr=1e-3)",
      "alf.optimizers.Adam(lr=5e-4)"
    ],
    "DdpgAlgorithm.critic_optimizer": [
      "alf.optimizers.Adam(lr=1e-3)",
      "alf.optimizers.Adam(lr=5e-4)"
    ],
    "OneStepTDLoss.td_error_loss_fn": [
      "alf.utils.losses.element_wise_huber_loss",
      "alf.utils.losses.element_wise_squared_loss"
    ]
  }
}
