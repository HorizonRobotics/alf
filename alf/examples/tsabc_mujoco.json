        "TsabcAlgorithm.beta_lb": "[0.5, 1.]",
        "TsabcAlgorithm.common_td_target": "[True]",
        "TsabcAlgorithm.use_basin_mean_for_target_critic": "[True]",
        "TrainerConfig.random_seed": "list(range(3))"
    }
}
